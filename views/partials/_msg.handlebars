{{#if sucess_msg}}
  <div class="container mt-5">
    <div class="row justify-content-center text-center">
      <div class="alert alert-success w-50 m-0">{{sucess_msg}}</div>
    </div>
  </div>
{{/if}}

{{#if error_msg}}
  <div class="container mt-5">
    <div class="row justify-content-center text-center">
      <div class="alert alert-danger w-50 m-0">{{error_msg}}</div>
    </div>
  </div>
{{/if}}